    "word = input() # 문자열 입력 받기\n",
    "N = len(word) # 문자열의 길이를 N이라는 변수에 저장\n",
    "if N > 1 and N < 100 :   # 문자열의 범위 지정\n",
    "    for i in range(0, N): # 각각의 단어 출력\n",
    "        print(word[i], end=\"\")\n",
    "        if(i % 10 == 9) : # 10개씩 끊어서 출력하기 위해 줄나누기 삽입\n",
